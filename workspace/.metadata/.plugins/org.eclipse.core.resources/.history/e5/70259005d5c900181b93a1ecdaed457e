#ifndef LIBRARIES_TIMETRIGGER_ACTIONEVENT_H_
#define LIBRARIES_TIMETRIGGER_ACTIONEVENT_H_

#include "Arduino.h"
#include <functional>
#include "AbstractEvent.h"

class ActionEvent{

public:
	ActionEvent(String _actionName,std::function<void(AbstractEvent)> _func);
	boolean executeFunction(String arg);
	String getName();

private:
	String actionName;
	std::function<void(String)> func;
};


#endif /* LIBRARIES_TIMETRIGGER_BUTTONB_H_ */
