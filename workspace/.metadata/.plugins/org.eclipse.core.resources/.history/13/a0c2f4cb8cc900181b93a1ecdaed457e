#include "Arduino.h"
#include "ActionListenerHelper.h"

ActionListenerHelper::ActionListenerHelper(ActionEvent _actionEvents[]){
	actionEvents=_actionEvents;
}

static ActionEvent ActionListenerHelper::buildActionEvent(String eventName,std::function<void(void* arg)> _func){
	ActionEvent result=ActionEvent(eventName,_func);
	return result;
}

boolean ActionListenerHelper::executeActionEvent(String eventName,void* arg){

	for(int i=0;i<actionEvents;i++){

	}

	return true;
}

