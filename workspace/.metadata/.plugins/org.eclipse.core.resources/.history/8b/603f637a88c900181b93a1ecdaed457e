#include "Arduino.h"
#include "ActionEvent.h"

ActionEvent::ActionEvent(String _actionName,std::function<void(void)> _func){
	actionName=_actionName;
	func=_func;

}

ActionEvent::ActionEvent(String _actionName,std::function<void(String)> _func){
	actionName=_actionName;
	funcString=_func;
}

boolean ActionEvent::executeVoidFunction(){
	if(func!=NULL){
		func();
		return true;
	}
}

